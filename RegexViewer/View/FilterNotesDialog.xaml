<Window x:Class="RegexViewer.FilterNotesDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Unsaved Settings" Height="400" Width="604.945">
    <Grid>
        
        <Button Name="buttonSave" Content="Save" HorizontalAlignment="Left" Margin="10,0,10,10" VerticalAlignment="Bottom" Width="75" Click="buttonSave_Click" />
        <Button Name="buttonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="100,0,10,10" VerticalAlignment="Bottom" Width="75" Click="buttonCancel_Click" />
        <TextBox Name="textBoxFilterNotes" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" HorizontalAlignment="Stretch" Margin="10,10,10,50" VerticalAlignment="Stretch" Visibility="Visible" TextWrapping="WrapWithOverflow" Grid.ColumnSpan="2" AcceptsReturn="True" />
    </Grid>
</Window>